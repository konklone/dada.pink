    library(ddr)
    ddr_init()

    chicks <- arpeggidata(sqrt(ChickWeight$weight),
                          blip,
                          scale="Emajor",
                          bpm=200,
                          count=1/32)
    play(chicks)


    import pandas
    from ddpy import to_midi

    data = {'minor':[50, 52, 53, 55, 57, 58, 60, 62]}
    df = pandas.DataFrame(data)
    to_midi(df, 'minor_scale.mid')


